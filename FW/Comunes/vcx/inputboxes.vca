Lparameters nPadreId As Integer

Local lcAlias As String,;
	lcCommand As String

Local lnPadreId As Integer
Local llSigue As Boolean
Local lcFilterCriteria As String

lcAlias = ""

Try

	llSigue = .T.
	nPadreId = Int( nPadreId )

	If Used( This.cboNieto.cEntityCursor )
		llSigue = Evaluate( This.cboNieto.cEntityCursor + "." + This.ChildPK ) <> nPadreId
	Endif


	If llSigue

		This.cboNieto.Dato.RowSource = ""

		If Used( This.cboNieto.cEntityCursor )
			Use In Alias( This.cboNieto.cEntityCursor )
		Endif

		TEXT To lcFilterCriteria NoShow TextMerge Pretext 15
		<<This.ChildPK>> = <<nPadreId>>
		EndText
		
		This.cboNieto.PopulateCombo( lcFilterCriteria )

*!*			This.cboNieto.oEntity.GetAll( lcFilterCriteria )

*!*			lcAlias = This.cboNieto.oEntity.MainCursorName

*!*			If Used( lcAlias )

*!*				If Empty( This.cboNieto.Selectedfields )
*!*					This.cboNieto.Selectedfields = This.cboNieto.oEntity.cSelectorFields
*!*				Endif

*!*				If !Lower( This.GrandChildPK )$Lower( This.cboNieto.Selectedfields )
*!*					This.cboNieto.Selectedfields = This.cboNieto.Selectedfields + "," + This.GrandChildPK
*!*				Endif

*!*				If Empty( This.cboNieto.OrderByFields )
*!*					This.cboNieto.OrderByFields = Getwordnum( This.cboNieto.Selectedfields, 1, "," )
*!*				Endif

*!*				TEXT To lcCommand NoShow TextMerge Pretext 15
*!*				Select <<This.cboNieto.Selectedfields>>
*!*					from <<lcAlias>>
*!*					Where <<lcAlias>>.<<This.ChildPK>> = <<nPadreId>>
*!*					into Cursor <<This.cboNieto.cEntityCursor>>
*!*					order By <<This.cboNieto.OrderByFields>>
*!*					nofilter Readwrite
*!*				ENDTEXT

*!*				&lcCommand

*!*				Select Alias( This.cboNieto.cEntityCursor )

*!*				TEXT To lcCommand NoShow TextMerge Pretext 15
*!*				Locate for <<This.GrandChildPK>> = Evaluate( This.cboNieto.Dato.ControlSource )
*!*				ENDTEXT


*!*				&lcCommand

*!*				If Eof()
*!*					Select Alias( This.cboNieto.CursorName )
*!*					Replace (This.GrandChildPK) With 0
*!*				Endif

*!*				If Empty( This.cboNieto.Dato.RowSource )
*!*					This.cboNieto.Dato.RowSource = This.cboNieto.cEntityCursor + "." + This.cboNieto.Selectedfields
*!*				Endif

*!*			Endif

	Endif


	This.cboNieto.Enabled = !Empty( nPadreId )

	This.cboNieto.Dato.Refresh()

Catch To oErr
	oErr.Message = oErr.Message + " (Combo " + This.cboNieto.Name + ")"
	Local loError As ErrorHandler Of "Tools\ErrorHandler\Prg\ErrorHandler.prg"
	loError = Newobject( "ErrorHandler", "ErrorhandlerPrg\ErrorHandler.prg" )
	loError.Process( oErr )

Finally

	If Used( lcAlias )
		Use In Alias( lcAlias )
	Endif

Endtry

ENDPROC
