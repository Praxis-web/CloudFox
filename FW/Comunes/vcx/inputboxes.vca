PROCEDURE autosetup

Lparameters tcStr As String

Local lnLen As Integer
Local lnTop As Integer
Local lnWidth As Integer
Local lcLabelName As String
Local loError As ErrorHandler Of "Tools\ErrorHandler\Prg\ErrorHandler.prg"
Try
    With This As iberrormessage Of FW\Comunes\vcx\inputboxes.vcx
        If Empty( tcStr )
            tcStr = ""
        Endif

        lnLen = Alines( laLines, tcStr, 1 )
        lnTop = .nGap
        lnWidth = .nGap
        For i = 1 To lnLen
            If Len(laLines[ i ]) < 255
                lcLabelName = "lbl" + Transform( i )
                .AddObject( lcLabelName, "prxLabel" )
                With (.&lcLabelName) As Label
                    .Top = lnTop
                    .Left = Thisform.nGap
                    .Caption = laLines[ i ]
                    .FontBold = Thisform.FontBold
                    .ForeColor = Thisform.ForeColor
                    .FontSize = Thisform.FontSize
                    If ! Empty( .Caption )
                        .AutoFit(.5)
                    Endif
                    .Alignment = 2
                    .Visible = .T.
                    lnTop = lnTop 	+ .Height
                    lnWidth = Max( .Width, lnWidth )
                Endwith

            Else
                lcLabelName = "edt" + Transform( i )
                .AddObject( lcLabelName, "prxEditBox" )
                With (.&lcLabelName) As EditBox
                    .Top = lnTop
                    .Left = Thisform.nGap
                    .Value = laLines[ i ]
                    .FontBold = Thisform.FontBold
                    .ForeColor = Thisform.ForeColor
                    .FontSize = Thisform.FontSize
*!*	                    If ! Empty( .Value )
*!*	                        .AutoFit(.5)
*!*	                    Endif
                    .Alignment = 2
                    .Visible = .T.
                    .Height = 120

                    lnTop = lnTop 	+ .Height
*!*	                    lnWidth = Max( .Width, lnWidth )
                Endwith

            Endif


        Endfor
        For i = 1 To lnLen
        	Try

	            lcLabelName = "lbl" + Transform( i )
	            With (.&lcLabelName) As Label
	                .Width = lnWidth
	            Endwith
        		

        	Catch To oErr
	            lcLabelName = "edt" + Transform( i )
	            With (.&lcLabelName) As EditBox
	                .Width = lnWidth
	            Endwith

        	Finally

        	EndTry
        Endfor
        .Width = lnWidth + ( 2 * .nGap )
        .cmdAceptar.Move( Abs( .Width - .cmdAceptar.Width )/2, lnTop + .nGap )
        .Height = .cmdAceptar.Top + .cmdAceptar.Height + .nGap
    Endwith
Catch To oErr
    loError = This.oError
    loError.Process( oErr )
    Throw loError
Finally
    loError = Null
Endtry


ENDPROC
