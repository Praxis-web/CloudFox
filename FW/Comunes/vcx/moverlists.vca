	lcCommand = ""


	Do Case
		Case nState = 0 && Enter
			oSource.DragIcon = Addbs( FL_IMAGE ) + "Cur\" + This.Parent.Parent.CanDropIcon

		Case nState = 1 && Leave
			oSource.DragIcon = Addbs( FL_IMAGE ) + "Cur\" + This.Parent.Parent.NoDropIcon

