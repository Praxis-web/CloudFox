[START METHODS]
PROCEDURE Destroy
This.oButtons = Null
ENDPROC
PROCEDURE Init

Bindevent( _Screen.oApp, "SynchronizeButtons", This, "SetButtons" )
ENDPROCPROCEDURE execute
Lparameters tcMethod, tcParameters

* DAE 2009-04-07

Local loControl
Local lcParameters As String
* If there is an active form and it has the method defined
If Type( "_Screen.ActiveForm" ) = "O" And Pemstatus( _Screen.ActiveForm, tcMethod, 5 )
	* If there is an active control,
	* and it's a text or edit or checkbox,
	* it gets focus to force the value update
	If Type( "_Screen.ActiveForm.ActiveControl" ) = "O"
		loControl = _Screen.ActiveForm.ActiveControl
		If Inlist( loControl.BaseClass, "Textbox", "Editbox", "Checkbox" )
			loControl.SetFocus()
		Endif
	Endif
	* Forces the parameters string
	lcParameters = Iif( Empty( tcParameters ), "", "( "+ tcParameters +" )" )
	* Execute the method
	_Screen.ActiveForm.&tcMethod &lcParameters
Endif

ENDPROC
PROCEDURE obuttons_access

With This As prxtoolbar Of fw\comunes\vcx\prxtoolbar.vcx
    If Vartype( .oButtons ) # "O"
        .oButtons = .oGlobalSettings.oApp.oButtons
    Endif
Endwith
Return This.oButtons

ENDPROC
PROCEDURE oglobalsettings_access

With This As prxtoolbar Of fw\comunes\vcx\prxtoolbar.vcx
    If Vartype( .oGlobalSettings ) # "O"
        * DAE 2009-07-30(18:08:14)
        * .oGlobalSettings = Newobject( 'GlobalSettings', 'GlobalSettings.prg' )
        .oGlobalSettings = NewGlobalSettings()

    EndIf && Vartype( .oGlobalSettings ) # "O"

Endwith

Return This.oGlobalSettings

ENDPROC
PROCEDURE setbuttons
Local loError As ErrorHandler Of "Tools\ErrorHandler\Prg\ErrorHandler.prg"
Try
	With This As prxtoolbar Of fw\comunes\vcx\prxtoolbar.vcx
		If .lIsOk
			.CmdClose.Enabled 	= .oButtons.Close
			.CmdDelete.Enabled 	= .oButtons.Delete
			.cmdEdit.Enabled 	= .oButtons.Edit
			.CmdNew.Enabled 	= .oButtons.New
			.CmdOpen.Enabled 	= .oButtons.Open
			.cmdReport.Enabled 	= .oButtons.Report
			.CmdSave.Enabled 	= .oButtons.Save
		Endif
	EndWith
	
Catch To oErr
	loError = Newobject( "ErrorHandler", "ErrorhandlerPrg\ErrorHandler.prg" )
	loError.Process( oErr )
	This.lIsOk = .F.
Finally
	loError = Null
Endtry

ENDPROC
