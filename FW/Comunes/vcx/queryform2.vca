Lparameters nKeyCode, nShiftAltCtrl

Do Case
	Case nKeyCode = 28
		Private puGrup As Variant,;
			puNume As Variant,;
			pnId As Integer,;
			pcOrden as String
			
		pcOrden = "CODIGO"	
