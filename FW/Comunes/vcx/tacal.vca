PROCEDURE rrcmonth
LPARAMETERS tcDate, tcLanguage
LOCAL lcRetVal AS String
lcRetVal = ""
DO CASE
	CASE tcLanguage = "ESP"
		DO CASE
			CASE MONTH( tcDate ) = 1
				lcRetVal = "Enero"
			CASE MONTH( tcDate ) = 2
				lcRetVal = "Febrero"
			CASE MONTH( tcDate ) = 3
				lcRetVal = "Marzo"
			CASE MONTH( tcDate ) = 4
				lcRetVal = "Abril"
			CASE MONTH( tcDate ) = 5
				lcRetVal = "Mayo"
			CASE MONTH( tcDate ) = 6
				lcRetVal = "Junio"
			CASE MONTH( tcDate ) = 7
				lcRetVal = "Julio"
			CASE MONTH( tcDate ) = 8
				lcRetVal = "Agosto"
			CASE MONTH( tcDate ) = 9
				lcRetVal = "Septiembre"
			CASE MONTH( tcDate ) = 10
				lcRetVal = "Octubre"
			CASE MONTH( tcDate ) = 11
				lcRetVal = "Noviembre"
			CASE MONTH( tcDate ) = 12
				lcRetVal = "Diciembre"
		ENDCASE
	OTHERWISE
		lcRetVal = CMONTH( tcDate )
ENDCASE
RETURN lcRetVal
ENDPROC
