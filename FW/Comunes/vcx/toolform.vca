PROCEDURE fillgrid
Lparameters tcAlias As String, lnModule_id As Integer


Try

	* Establezco el ControlSource activo
	Thisform.gridPanel.GridControlSource = tcAlias

	Thisform.LockScreen = .T.

	* Método de Usuario
	* Generalmente, se blanquean las propiedades ControlSource
	This.BeforeFillGrid()
	Thisform.gridPanel.Grid.RecordSource = ""

	* Si la tabla no existe, la cargo

	If !Used( tcAlias )
		Select Alias( Thisform.ModulesCursor )

		Locate For Modulo_ID = lnModule_id

		If !Eof()
			This.oEntity.GetAllTables( lnModule_id, tcAlias )
		Endif
	Endif

	Select Alias( tcAlias )
	Locate

	Thisform.gridPanel.Grid.RecordSource = tcAlias

	* Método de usuario
	* Generalmente se vinculan las propiedades ControlSource
	This.AfterFillGrid()

	Thisform.gridPanel.Grid.Refresh()
	Thisform.LockScreen = .F.


	*!*		If Used( tcAlias )
	*!*			Select Alias( tcAlias )
	*!*			Browse

	*!*		EndIf


Catch To oErr
	Local loError As ErrorHandler Of "Tools\ErrorHandler\Prg\ErrorHandler.prg"
	loError = Newobject( "ErrorHandler", "ErrorhandlerPrg\ErrorHandler.prg" )
	loError.Process( oErr )

Finally

Endtry



ENDPROC
