		With This.OptionGroup.Buttons[ lnItem ]
			.Caption = Alltrim( Evaluate( tcAlias + ".Descripcion" ) )
			.Module_Id = Evaluate( tcAlias + ".Modulo_Id" )
			.CursorName = Sys(2015)
		EndWith
		
	Endscan

	This.OptionGroup.Top = This.nTopPadding
	This.optionGroup.Left = ( This.Width - This.OptionGroup.Width ) / 2

Catch To oErr
