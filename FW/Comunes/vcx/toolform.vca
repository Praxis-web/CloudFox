PROCEDURE loadmodules
#INCLUDE "FW\Tieradapter\Include\TA.h"

Local llOk As Boolean
Local lcFilterCriteria As String

Try

	This.oError.Ctracelogin = ""
	This.oError.Cremark = ""

	With Thisform As tfToolForm Of "FW\Comun\Vcx\ToolForm.vcx"
		.LaunchSelectorAfterInit = .F.

		.BindControls = .F.

		If .lIsOk
			* Acá debería cargarse lcFilterCriteria con los permisos del usuario
			* El proceso sólo trará los módulos marcados como ACTIVOS.
			lcFilterCriteria = "1 > 0"

			.oEntity.GetAllModulos( lcFilterCriteria )

		Endif

		If .lIsOk
			.ModulesCursor = Alias()
			.ModulesSelectorPanel.LoadModules( .ModulesCursor  )
		Endif

		If .lIsOk
			.nStatus = IS_EDITING
		Endif

	Endwith

Catch To oErr
	Throw oErr

Finally
	llOk = This.lIsOk
	This.lIsOk = .T.

Endtry

Return llOk

ENDPROC
