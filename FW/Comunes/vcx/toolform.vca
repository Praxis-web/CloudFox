PROCEDURE Click
Try

	Select Alias( "cModulo" )
	
	Browse
	
	Locate
	Scan 	
		lcTableAlias= "_"+Any2Char( cModulo.Modulo_ID )+"_Table"
		
		This.oEntity.GetAllTables( cModulo.Modulo_ID, lcTableAlias )
		
		Browse last
		
		Select Alias( "cModulo" )
	EndScan


Catch To oErr
	Local loError As ErrorHandler Of "Tools\ErrorHandler\Prg\ErrorHandler.prg"
	loError = Newobject( "ErrorHandler", "ErrorhandlerPrg\ErrorHandler.prg" )
