[ RECORD]
[PLATFORM] WINDOWS 
[UNIQUEID] _2XA0QYCZG
[CLASS] spinner
[BASECLASS] spinner
[OBJNAME] spnInicial
[PARENT] FRMHASARUTILS.PageFrame.CloseByNumber
[START PROPERTIES]
Height = 24
Left = 153
Top = 44
Width = 120
Name = "spnInicial"
[END PROPERTIES]
[START METHODS]
PROCEDURE Init
#INCLUDE "FW\Hasar\Include\Hasar.h"

Local loHasar As "Hasar.Fiscal.1"

Try
	loHasar = NewHasar()
	

	Try

		If lHasar
			loHasar.LeerMemoriaDeTrabajo() 	
		EndIf
	
		This.Value = Int( Val( loHasar.Respuesta( W_ULTIMO_Z ) ) )		
	
		This.SpinnerHighValue 	= This.Value
		This.KeyboardHighValue 	= 999999999


	Catch To oErr
		This.Value = 1
		
	Finally

	EndTry

	This.SpinnerLowValue = 1
	This.KeyboardLowValue = 1
	
Catch To oErr
	Local loError As PrxErrorHandler Of "fw\ErrorHandler\prxErrorHandler.prg"

	loError = Newobject( "PrxErrorHandler", "prxErrorHandler.prg" )
	loError.Process( oErr )

Finally
	loHasar = Null

Endtry

ENDPROC
[END METHODS]

