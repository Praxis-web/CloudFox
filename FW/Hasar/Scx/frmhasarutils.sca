PROCEDURE dailyclose
Lparameters tcParam As Character
#INCLUDE "FW\Hasar\Include\Hasar.h"

Local loHasar As "Hasar.Fiscal.1"
Local ldDateTime As Datetime
Local lnHoras as Integer
Local lnMinutos as Integer
Local lnSegundos as Integer


Try

	Thisform.PageFrame.ActivePage = 2

	Do Case
		Case tcParam = "Z"

			loHasar = NewHasar()
			
			If lHasar
				ldDateTime = loHasar.FechaHoraFiscal	
				
			Else
				ldDateTime = Datetime()
				
			Endif
			
			lnHoras = Val( Substr( Ttoc( ldDateTime, 2 ), 1, 2 ))
			lnMinutos  = Val( Substr( Ttoc( ldDateTime, 2 ), 4, 2 ))
			lnSegundos = Val( Substr( Ttoc( ldDateTime, 2 ), 7, 2 ))

			ThisForm.PageFrame.Page2.dtNewDate.txtDate.Value = Ttod( ldDateTime )
			Thisform.PageFrame.Page2.spnHoras.Value = lnHoras
			Thisform.PageFrame.Page2.spnMinutos.Value = lnMinutos
			Thisform.PageFrame.Page2.spnSegundos.Value = lnSegundos
			
			Thisform.Caption = "Cierre Diario 'Z'"
			Thisform.PageFrame.Page2.dtNewDate.Visible = .T.
			Thisform.PageFrame.Page2.chkDateSetup.Visible = .T.
			Thisform.PageFrame.Page2.chkTimeSetup.Visible = .T.
			Thisform.PageFrame.Page2.spnHoras.Visible = .T.
			Thisform.PageFrame.Page2.spnMinutos.Visible = .T.
			Thisform.PageFrame.Page2.spnSegundos.Visible = .T.

		Case tcParam = "X"
			Thisform.Caption = "Cierre Diario 'X'"
			Thisform.PageFrame.Page2.dtNewDate.Visible = .F.
			Thisform.PageFrame.Page2.chkDateSetup.Visible = .F.
			Thisform.PageFrame.Page2.chkTimeSetup.Visible = .F.
			Thisform.PageFrame.Page2.spnHoras.Visible = .F.
			Thisform.PageFrame.Page2.spnMinutos.Visible = .F.
			Thisform.PageFrame.Page2.spnSegundos.Visible = .F.

		Otherwise

	Endcase

	Thisform.PageFrame.Page2.Label1.Caption = Thisform.Caption

	Thisform.cComando = "DailyClose"
	Thisform.cParametros = tcParam


Catch To oErr
	Local loError As PrxErrorHandler Of "fw\ErrorHandler\prxErrorHandler.prg"

	loError = Newobject( "PrxErrorHandler", "prxErrorHandler.prg" )
	loError.Process( oErr )
	Throw loError

Finally
	loHasar = Null

Endtry

ENDPROC
PROCEDURE doclosebydate
Lparameters tcParam As Character

Local loHasar As "Hasar.Fiscal.1"
Local ldDesde As Date
Local ldHasta As Date
Local llGlobal As Boolean
Local llOk As Boolean

Try

	llOk = .T.
	loHasar = NewHasar()

	Wait Window "Imprimiendo " + Thisform.Caption Nowait

	ldDesde = Thisform.PageFrame.CloseByDate.FechaInicial.txtDate.Value
	ldHasta = Thisform.PageFrame.CloseByDate.FechaFinal.txtDate.Value
	llGlobal = ( tcParam = "T" )

	If ldDesde > ldHasta
		llOk = .F.

		TEXT To lcCommand NoShow TextMerge Pretext 03
		La Fecha Final Debe Ser Mayor A La Fecha Inicial
		Fecha Inicial: <<ldDesde>>
		Fecha Final: <<ldHasta>>
		ENDTEXT

		Warning( lcCommand )

		Thisform.PageFrame.CloseByDate.FechaFinal.txtDate.Value = ldDesde
		Thisform.PageFrame.CloseByDate.FechaFinal.txtDate.SetFocus()

	Else
		If ldHasta > ldDesde
			loHasar.ReporteZPorFechas( ldDesde, ldHasta, llGlobal )

		Else
			loHasar.ReporteZIndividualPorFecha( ldDesde )

		Endif

	Endif



Catch To oErr
	Local loError As PrxErrorHandler Of "fw\ErrorHandler\prxErrorHandler.prg"

	loError = Newobject( "PrxErrorHandler", "prxErrorHandler.prg" )
	loError.Process( oErr )
	Throw loError

Finally
	loHasar = Null
	If llOk
		Wait Clear
		Thisform.PageFrame.ActivePage = 1
	Endif

Endtry

ENDPROC
PROCEDURE doclosebynumber
Lparameters tcParam As Character

Local loHasar As "Hasar.Fiscal.1"
Local lnDesde as Integer
Local lnHasta as Integer
Local llGlobal as Boolean
Local llOk As Boolean

Try

	llOk = .T.

	loHasar = NewHasar()

	Wait Window "Imprimiendo " + Thisform.Caption Nowait

	lnDesde = ThisForm.PageFrame.CloseByNumber.spnInicial.Value
	lnHasta = ThisForm.PageFrame.CloseByNumber.spnFinal.Value
	llGlobal = ( tcParam = "T" )
	
	loHasar.ReporteZPorNumeros( lnDesde, lnHasta, llGlobal )
	
	If lnDesde > lnHasta
		llOk = .F.

		TEXT To lcCommand NoShow TextMerge Pretext 03
		El Número Final Debe Ser Mayor Al Número Inicial
		Número Inicial: <<lnDesde>>
		Número Final: <<lnHasta>>
		ENDTEXT

		Warning( lcCommand )

		ThisForm.PageFrame.CloseByNumber.spnFinal.Value = lnDesde
		ThisForm.PageFrame.CloseByNumber.spnFinal.SetFocus()

	Else
		If lnHasta > lnDesde
			loHasar.ReporteZPorNumeros( lnDesde, lnHasta, llGlobal )

		Else
			loHasar.ReporteZIndividualPorNumero( lnDesde )

		Endif

	Endif
	


Catch To oErr
	Local loError As PrxErrorHandler Of "fw\ErrorHandler\prxErrorHandler.prg"

	loError = Newobject( "PrxErrorHandler", "prxErrorHandler.prg" )
	loError.Process( oErr )
	Throw loError

Finally
	loHasar = Null
	If llOk
		Wait Clear
		Thisform.PageFrame.ActivePage = 1
	Endif

Endtry

ENDPROC
PROCEDURE dodailyclose
Lparameters tcParam As Character

Local loHasar As "Hasar.Fiscal.1"
Local lcDateTime As String
Local lcFecha As String
Local lcHora As String

Try

	loHasar = NewHasar()

	Wait Window "Imprimiendo " + Thisform.Caption Nowait

	Do Case
		Case tcParam = "Z"

			loHasar.ReporteZ()

			If Thisform.PageFrame.Page2.chkSetSystem.Value ;
					Or Thisform.PageFrame.Page2.chkDateSetup.Value ;
					Or Thisform.PageFrame.Page2.chkTimeSetup.Value

				If Thisform.PageFrame.Page2.chkSetSystem.Value
					lcDateTime = Ttoc( Datetime(), 1 )
					lcFecha = Substr( lcDateTime, 3, 6 )
					lcHora = Substr( lcDateTime, 9, 6 )

				Else
					lcDateTime = Ttoc( loHasar.FechaHoraFiscal, 1 )
					lcFecha = Substr( lcDateTime, 3, 6 )
					lcHora = Substr( lcDateTime, 9, 6 )

					If Thisform.PageFrame.Page2.chkDateSetup.Value
						lcFecha = Substr( Dtos( Thisform.PageFrame.Page2.dtNewDate.txtDate.Value ), 3, 6 )
					Endif

					If Thisform.PageFrame.Page2.chkTimeSetup.Value
						lcHora = StrZero( Thisform.PageFrame.Page2.spnHoras.Value, 2 )
						lcHora = lcHora +  StrZero( Thisform.PageFrame.Page2.SpnMinutos.Value, 2 )
						lcHora = lcHora +  StrZero( Thisform.PageFrame.Page2.SpnSegundos.Value, 2 )
					Endif

				EndIf
				
				SetDateTime( lcFecha, lcHora )
				
			Endif

		Case tcParam = "X"
			loHasar.ReporteX()

		Otherwise

	Endcase

Catch To oErr
	Local loError As PrxErrorHandler Of "fw\ErrorHandler\prxErrorHandler.prg"

	loError = Newobject( "PrxErrorHandler", "prxErrorHandler.prg" )
	loError.Process( oErr )
	Throw loError

Finally
	loHasar = Null
	Wait Clear
	Thisform.PageFrame.ActivePage = 1

Endtry

ENDPROC
