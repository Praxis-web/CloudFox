PROCEDURE synchronizebuttons
Local lcCommand as String
Local llOk as Boolean
Local lnGap as Integer

Try

	lcCommand = ""

	llOk = .T.
	lnGap = 12
	
	ThisForm.PageFrame.Tabs = .F.
	ThisForm.PageFrame.Top = 0
	ThisForm.PageFrame.Left = 0
	
	

	ThisForm.Shape1.Top = Thisform.Height - ThisForm.Shape1.Height - lnGap
	ThisForm.Shape1.Left = lnGap
	ThisForm.Shape1.Width = Thisform.Width - ( lnGap * 2 )
	ThisForm.Shape1.Height = ThisForm.cmdCancelar.Height + ( lnGap * 2 )
	
	ThisForm.cmdCancelar.Visible = .T.
	ThisForm.cmdCancelar.Top = ThisForm.Shape1.Top + lnGap
	ThisForm.cmdCancelar.Left = ThisForm.Shape1.Left + lnGap
	
	ThisForm.cmdAceptar.Visible = .T.
	ThisForm.cmdAceptar.Top = ThisForm.cmdCancelar.Top
	ThisForm.cmdAceptar.Left = Thisform.Width - ( lnGap * 2 ) - ThisForm.cmdAceptar.Width

	ThisForm.PageFrame.Height = ThisForm.Shape1.Top - lnGap
	ThisForm.PageFrame.Width = Thisform.Width
	
	ThisForm.cmdVolver.Visible = .F.
	ThisForm.cmdProcess.Visible = .F.
	
	ThisForm.cmdVolver.Top = ThisForm.cmdCancelar.Top
	ThisForm.cmdVolver.Left = ThisForm.cmdCancelar.Left
	
	ThisForm.cmdProcess.Top = ThisForm.cmdAceptar.Top
	ThisForm.cmdProcess.Left = ThisForm.cmdAceptar.Left

Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = _NewObject ( 'ErrorHandler', 'ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally


EndTry

