Local loModelo As oTipo_Comprobante Of "Clientes\Archivos\prg\Tipo_Comprobante.prg"

Try

	lcCommand = ""
	loModelo = This.oModelo
	llFiltraPorComprobante  = loModelo.lFiltraPorComprobante
