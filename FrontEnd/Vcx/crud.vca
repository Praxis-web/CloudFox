		Bindevent( Thisform, 'InitializeControls', This, 'Initialize', 1 )
	Endif
	
	This.Width 	= 240
	This.Height = 84
	This.Crud_Shape.BackStyle = 1
	This.Crud_Shape.BackColor = RGB(245,245,255)
	
	
	This.BorderWidth 			= 0
