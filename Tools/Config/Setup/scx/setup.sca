[ RECORD]
[PLATFORM] WINDOWS 
[UNIQUEID] _66P0KTWIP
[CLASS] combobox
[BASECLASS] combobox
[OBJNAME] cboSeleccionar
[PARENT] Setup
[START PROPERTIES]
Height = 24
Left = 276
Top = 144
Width = 432
Name = "cboSeleccionar"
[END PROPERTIES]
[START METHODS]
PROCEDURE Init
Local lcCommand as String
Local i as Integer

Try

	lcCommand = ""
	
	This.BoundColumn 	= 2
	This.BoundTo 		= .T.
	This.ColumnCount 	= 1
	This.RowSourceType 	= 0
	This.RowSource 		= ""
	This.Style			= 2
	This.Sorted 		= .F.
	
	If !Used( "Urls" )
		Use Urls Shared In 0
	Endif
	
	Text To lcCommand NoShow TextMerge Pretext 15
	Select *
		From Urls
		Where !Deleted()
		Order By Orden,Alias
		Into Cursor cUrsl ReadWrite
	EndText

	&lcCommand
	lcCommand = ""
	
	Locate
	
	Scan 	
	
		This.AddItem( cUrsl.Alias )
		This.List( This.NewIndex, 2 ) = Transform( cUrsl.Id )

	EndScan
	
Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = NewObject( 'ErrorHandler', 'Tools\ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally


EndTry

ENDPROC
PROCEDURE InteractiveChange
Local lcCommand As String
Local lnId As Integer

Try

	lcCommand = ""

	If !Used( "Urls" )
		Use Urls Shared In 0
	Endif

	Select Urls
	Locate For Id = Val( This.Value )

	If Found()
		Thisform.txtURL.Value = Alltrim( Urls.Url )
	Endif


Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = Newobject( 'ErrorHandler', 'Tools\ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally


Endtry

ENDPROC
[END METHODS]

