PROCEDURE Load
Thisform.BindControls = .F.

Set Cursor On
Set Date DMY
Set Century On
Set Cpdialog Off
Set Safety Off
Set TablePrompt Off
Set TableValidate To 0
Set Deleted On

Set Datasession To 1
ENDPROC
PROCEDURE Unload

Thisform.oParam = Null
Thisform.oBiz = Null

Close Databases all
Clear Events
ENDPROC
PROCEDURE cancelar
Thisform.Release()
ENDPROC
PROCEDURE editar
Local lcCommand as String

Try

	lcCommand = ""
	Thisform.oBiz.Editar()

Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = NewObject( 'ErrorHandler', 'Tools\ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally


EndTry

ENDPROC
PROCEDURE grabar
Thisform.oBiz.Grabar( Thisform.oParam )
Thisform.Cancelar()

ENDPROC
PROCEDURE inicializarcontroles
Local lcCommand As String
Local loCB As ComboBox

Try

	lcCommand = ""

	Thisform.txtURL.ControlSource = "Thisform.oParam.cUrl"

	loCB = Thisform.cboSeleccionar
	For i = 1 To loCB.ListCount
		Select Urls
		Locate For Id = Val( loCB.List( i, 2 ) )

		If Found()
			If Alltrim( Urls.Url ) = Thisform.txtURL.Value
				loCB.Selected( i ) = .T.
			Endif
		Endif
	Endfor


Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = Newobject( 'ErrorHandler', 'Tools\ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally


Endtry

ENDPROC
PROCEDURE loadsetup
Local lcCommand as String
Local loBiz As oSetup Of "Tools\Config\Setup\prg\Setup.prg"

Try

	lcCommand 	= ""
	loBiz 		= Thisform.oBiz
	Thisform.oParam = loBiz.LoadSetup()

	Thisform.BindControls = .T.
	
Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = NewObject( 'ErrorHandler', 'Tools\ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally
	loBiz = Null

EndTry

ENDPROC
PROCEDURE obiz_access
Local lcCommand As String

Try

	lcCommand = ""
	If Vartype( Thisform.oBiz ) # "O"
		Thisform.oBiz = Newobject( "oSetup", "Tools\Config\Setup\prg\Setup.prg" )
	Endif

Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = Newobject( 'ErrorHandler', 'Tools\ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally


Endtry

Return Thisform.oBiz

ENDPROC
PROCEDURE probar
Thisform.oBiz.Probar( Thisform.oParam )
ENDPROC
PROCEDURE savesetup
Local lcCommand As String

Try

	lcCommand = ""
	loBiz = Thisform.oBiz
	loBiz.oParam = Thisform.oParam
	loBiz.SaveSetup(  )


Catch To loErr
	Local loError As ErrorHandler Of 'Tools\ErrorHandler\Prg\ErrorHandler.prg'
	loError = Newobject( 'ErrorHandler', 'Tools\ErrorHandler\Prg\ErrorHandler.prg' )
	loError.cRemark = lcCommand
	loError.Process ( m.loErr )
	Throw loError

Finally
	loBiz = Null

Endtry

ENDPROC
